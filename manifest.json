{
  "manifest_version": 2,
  "name": "S",
  "version": "1.0.7",
  "description": "This extension verifies the code exists somewhere on this page",

  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["tabs", "http://localhost:8000/*", "storage"]
}
